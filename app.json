{
  "expo": {
    "name": "MagicTales-AI",
    "slug": "MagicTales-AI",
    "version": "1.2.34",
    "orientation": "portrait",
    "icon": "./assets/magicAI-trans.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/magicAI-trans.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.smartsoftware.sas"
    },
    "android": {
      "versionCode": 139,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "useNextNotificationsApi": true,
      "package": "com.sparktech.magicai",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyC4fsrgciACjhUacg9umvRHflrJJHFovdY"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.9.24",
            "composeCompilerVersion": "1.5.14"
          }
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "9a9590fa-c4fd-48d8-b1bc-0f41eec8d292"
      }
    }
  }
}
